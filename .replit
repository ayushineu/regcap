modules = ["python-3.11"]

[nix]
channel = "stable-24_05"

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "New App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Simplified App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "RegCap"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Simplified JS Fix"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "RegCap Minimal"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Simple JS Fix"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Deployment"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Deploy"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Deployment App"

[[workflows.workflow]]
name = "New App"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python new_flask_app.py --port 5001"
waitForPort = 5001

[[workflows.workflow]]
name = "Simplified App"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python simplified_app.py --port 5002"
waitForPort = 5002

[[workflows.workflow]]
name = "RegCap"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py --port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "Simplified JS Fix"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python simplified_app_js_fix.py"
waitForPort = 5003

[[workflows.workflow]]
name = "RegCap Minimal"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python regcap_minimal.py"
waitForPort = 5000

[[workflows.workflow]]
name = "Simple JS Fix"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python simple_js_fix.py"
waitForPort = 5005

[[workflows.workflow]]
name = "Deployment"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python deployment.py --port=5000"
waitForPort = 5000

[[workflows.workflow]]
name = "Deploy"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python deploy.py"
waitForPort = 8080

[[workflows.workflow]]
name = "Deployment App"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app_deploy.py"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3000

[[ports]]
localPort = 5002
externalPort = 3001

[[ports]]
localPort = 5003
externalPort = 3002

[[ports]]
localPort = 5005
externalPort = 3003

[[ports]]
localPort = 5010
externalPort = 4200

[[ports]]
localPort = 8080
externalPort = 8080
